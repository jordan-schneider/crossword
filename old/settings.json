{
  "board": {
    "cell-size": 33,
    "fill": {
      "correct": "green",
      "default": "white",
      "empty": "black",
      "incorrect": "red",
      "selected": "light blue",
      "selected-letter": "yellow"
    },
    "font-family": "tkDefaultFont",
    "font-color": "black"
  },
  "controls": {
    "arrow-movement": [
      "absolute",
      "interpreted"
    ],
    "on-arrow": [
      "switch direction before moving",
      "only switch direction",
      "switch direction and move"
    ],
    "on-return": [
      "go to next word",
      "stay in cell"
    ],
    "on-filled-cell": [
      "skip cell",
      "stay in cell"
    ],
    "on-last-letter": [
      "go to next word",
      "go to first cell",
      "stay in cell"
    ],
    "on-space": [
      "go to next cell",
      "switch direction"
    ],
    "on-tab": [
      "go to next word"
    ],
    "on-backspace": [
      "go to last cell",
      "stay in cell"
    ]
  },
  "style": {
    "time": {
      "fg": "black",
      "font": [
        "tkDefaultFont",
        15
      ]
    },
    "clue": {
      "fg": "black",
      "font": [
        "tkDefaultFont",
        15
      ]
    },
    "font": {
      "size-modifier": 0
    },
    "author": {
      "fg": "black",
      "font": [
        "tkDefaultFont",
        17
      ]
    },
    "label": {
      "fg": "black",
      "font": [
        "tkDefaultFont",
        15
      ]
    },
    "border": {
      "fill": "black"
    },
    "list": {
      "fg": "black",
      "selectbackground": "light blue",
      "width": 35,
      "font": [
        "tkDefaultFont",
        10
      ]
    },
    "title": {
      "fg": "black",
      "font": [
        "tkDefaultFont",
        17
      ]
    }
  }
}